<!-- Use stable and unique id's!-->
<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:uxap="sap.uxap" 
	xmlns:reuse="vwks.nlp.s2p.mm.reuse.lib.documentHistory">
	<uxap:ObjectPageSection 
		id="idDocHistorySection" 
		title="{i18n>DocumentHistorySection}" 
		titleUppercase="false" >
		<uxap:ObjectPageSubSection id="idDocHistorySubsection">
			<reuse:DocumentHistory 
				id="idDocumentHistory"
				application="MPRC_ITEM"
				items="{docHistory>/items}">
				<reuse:items>
					<core:Item id="idItem" key="{docHistory>key}" text="{docHistory>text}"/>
				</reuse:items>
			</reuse:DocumentHistory>
		</uxap:ObjectPageSubSection>
	</uxap:ObjectPageSection>
</core:FragmentDefinition>